<link href="//netdna.bootstrapcdn.com/bootstrap/3.2.0/css/bootstrap.min.css" rel="stylesheet" id="bootstrap-css">
<script src="//netdna.bootstrapcdn.com/bootstrap/3.2.0/js/bootstrap.min.js"></script>
<script src="//code.jquery.com/jquery-1.11.1.min.js"></script>
<link href="//netdna.bootstrapcdn.com/bootstrap/3.0.0/css/bootstrap.min.css" rel="stylesheet" id="bootstrap-css">
<script src="//netdna.bootstrapcdn.com/bootstrap/3.0.0/js/bootstrap.min.js"></script>
<script src="//code.jquery.com/jquery-1.11.1.min.js"></script>
<div class="row">
    <div class="col-sm-6 col-md-4 col-md-offset-4 col-sm-offset-3">
        <div id="success" class="alert alert-success" {{#if noMessages}}hidden{{/if}}>
            {{ successMsg }}
        </div>
    </div>
</div>

{{# each products}}
        <div class="row">
        {{# each this}}
            <div class="col-xs-18 col-sm-6 col-md-12">
              <div class="thumbnail">
                <img src="{{this.imagePath}}">
                  <div class="caption">
                    <h4>{{this.title}}</h4>
                    <p>{{this.description}}</p>

                    <a href="/add-to-cart/{{this._id}}" class="btn btn-default btn-xs pull-right" role="button">Add To Cart</a> <a href="#" class="btn btn-info btn-xs" role="button"><i class="fas fa-rupee-sign"></i>{{this.price}}</a>
                  </div>
              </div>
            </div>


    {{/each}}
      </div><!--/row-->

{{/each}}